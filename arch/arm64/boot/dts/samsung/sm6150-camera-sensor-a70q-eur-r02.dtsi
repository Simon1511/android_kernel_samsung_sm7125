			cam_vdig-supply = <&s2mpb03_l2>;
			cam_vana-supply = <&s2mpb03_l6>;
			cam_clk-supply = <&titan_top_gdsc>; 
			regulator-names = "cam_vaf", "cam_vio", "cam_vdig", "cam_vana", "cam_clk";
			rgltr-cntrl-support;
			rgltr-min-voltage = <2800000 1800000 1050000 2800000 0>;
			rgltr-max-voltage = <2800000 1800000 1050000 2800000 0>;
			rgltr-load-current = <200000 200000 200000 200000 0>;
			gpio-no-mux = <0>;
			pinctrl-names = "cam_default", "cam_suspend";
			pinctrl-0 = <&cam_sensor_mclk_0_active &cam_sensor_rear_active>;
			pinctrl-1 = <&cam_sensor_mclk_0_suspend &cam_sensor_rear_suspend>;
