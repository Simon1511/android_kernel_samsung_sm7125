#include "sm6150-sec-m40-swa-r00.dtsi"

&soc {
	
};

/* not use ufs */
&ufsphy_mem {
	status = "disabled";
};
&ufshc_mem {
	status = "disabled";
};
&ufs_ice {
	status = "disabled";
};

/* use eMMC */
&sdhc_1 {
	status = "ok";
};

&firmware {
	android {
		fstab {
			product {
				dev = "/dev/block/platform/soc/7c4000.sdhci/by-name/product";
			};
			vendor {
				dev = "/dev/block/platform/soc/7c4000.sdhci/by-name/vendor";
			};
		};
	};
};
