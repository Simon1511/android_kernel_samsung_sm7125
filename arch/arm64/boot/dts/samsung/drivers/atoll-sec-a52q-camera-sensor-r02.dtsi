#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include <dt-bindings/clock/qcom,camcc-atoll.h>

&reserved_memory {

};

&sensor_rear_macro {
		cam_vio-supply = <&s2mpb03_l3>;
		cam_vdig-supply = <&s2mpb03_l4>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vio", "cam_vdig", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 1200000 0>;
		rgltr-max-voltage = <1800000 1200000 0>;
		rgltr-load-current = <200000 200000 0>;

		// GPIO
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk4_active &rcam4_sensor_reset_active &rcam4_sensor_vana_active &cam_mipi_sel_active>;
		pinctrl-1 = <&cam_sensor_mclk4_suspend &rcam4_sensor_reset_suspend &rcam4_sensor_vana_suspend &cam_mipi_sel_suspend>;

		gpios = <&tlmm 16 0>, /* MCLK */
			<&tlmm 24 0>, /* RESET */
			<&tlmm 64 0>, /* VANA */
			<&tlmm 66 0>; /*MIPI SEL*/
};

&eeprom4 {
		cam_vio-supply = <&s2mpb03_l3>;
		cam_vdig-supply = <&s2mpb03_l4>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vio", "cam_vdig", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 1200000 0>;
		rgltr-max-voltage = <1800000 1200000 0>;
		rgltr-load-current = <200000 200000 0>;

		// GPIO
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk4_active &rcam4_sensor_reset_active &rcam4_sensor_vana_active &cam_mipi_sel_active>;
		pinctrl-1 = <&cam_sensor_mclk4_suspend &rcam4_sensor_reset_suspend &rcam4_sensor_vana_suspend &cam_mipi_sel_suspend>;

		gpios = <&tlmm 16 0>, // MCLK
			<&tlmm 24 0>, // RESET
			<&tlmm 64 0>, // VANA
			<&tlmm 66 0>; // MIPI SEL
};
