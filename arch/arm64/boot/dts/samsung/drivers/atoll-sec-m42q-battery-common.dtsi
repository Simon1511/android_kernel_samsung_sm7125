#include <dt-bindings/iio/qcom,spmi-vadc.h>

&qupv3_se4_i2c {
	status = "okay";
	sm5440@63{
		status = "okay";
		compatible = "siliconmitus,sm5440";
		reg = <0x63>;
		pinctrl-names = "default";

	};
};

&pm6150_vadc {
	bat_thm { /* AMUX_3 */
		label = "bat_thm";
		reg = <ADC_AMUX_THM3_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
	chg_thm { /* AMUX_1 */
		label = "chg_thm";
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

