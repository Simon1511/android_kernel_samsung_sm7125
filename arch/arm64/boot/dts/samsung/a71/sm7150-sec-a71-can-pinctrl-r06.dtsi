&soc {
	
};

&tlmm {
	/* s2mu106_fg i2c */
	s2mu106_fg_i2c: s2mu106_fg_i2c {
		s2mu106_fg_active: s2mu106_fg_active {
			mux {
				pins = "gpio102", "gpio101";
				function = "gpio";
			};

			config {
				pins = "gpio102", "gpio101";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	dc_irq_default: dc_irq_default {
		config {
			pins = "gpio57";
			drive-strength = <2>;
			bias-disable;
			input-enable;
		};
	};
};

