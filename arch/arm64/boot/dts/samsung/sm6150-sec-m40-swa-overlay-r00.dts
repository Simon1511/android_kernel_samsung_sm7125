/ {
	model = "Samsung M40 PROJECT Bring-Up (board-id,00)";
	compatible = "qcom,sm6150p-idp", "qcom,sm6150p", "qcom,idp";
	qcom,msm-id = <355 0x0>;
	qcom,board-id = <34 0>;
