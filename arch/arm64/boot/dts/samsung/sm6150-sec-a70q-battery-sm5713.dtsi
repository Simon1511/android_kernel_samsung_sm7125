&pm6150_vadc {
	bat_thm { /* BAT_THERM */
		label = "bat_thm";
		reg = <ADC_BAT_THERM_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
	chg_thm { /* AMUX_4 */
		label = "chg_thm";
		reg = <ADC_AMUX_THM4_PU2>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

