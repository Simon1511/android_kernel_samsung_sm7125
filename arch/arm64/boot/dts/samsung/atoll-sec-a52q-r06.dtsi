#include "atoll-sec-a52q-r05.dtsi"
#include "atoll-sec-a52q-pinctrl-r06.dtsi"

/delete-node/ &vibrator;

&soc {
	msm_vibrator {
		compatible = "vibrator";
		motor-vdd_type = <0>; /* 0: gpio, 1: pmic */
		motor-en = <&tlmm 57 0x0>;
		samsung,vib_type = "COINDC";
	};
};
