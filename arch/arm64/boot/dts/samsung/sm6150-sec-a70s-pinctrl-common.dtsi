&lpi_tlmm {
	gpio-reserved-ranges = <0 8>, <12 6>, <25 5>;
};

&pm6150_gpios {
	hall {
		hall_default: hall_default {
			pins = "gpio10";
			function = "normal";
			output-disable;
			input-enable;
			bias-disable;
			power-source = <0>;
		};
	};
	detect_conn_pm_setting: detect_conn_pm_setting {
		pins = "gpio8"; /* SUB_DET */
		bias-disable; /* NO PULL */
		input-enable;
	};
};

&pm6150l_gpios {
	grip_irq: grip-irq {
		pins = "gpio5";
		function = "normal";
		power-source = <0>;
		input-enable;
		bias-disable;
	};
	grip_input: grip-input {
		pins = "gpio5";
		function = "normal";
		power-source = <0>;
		input-enable;
		bias-disable;
	};
};


