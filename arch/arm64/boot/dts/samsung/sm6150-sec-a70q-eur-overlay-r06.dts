/ {
	model = "Samsung A70Q PROJECT Rev0.3 (board-id,06)";
	compatible = "qcom,sm6150p-idp", "qcom,sm6150p", "qcom,idp";
	qcom,msm-id = <355 0x0>;
	qcom,board-id = <34 6>;
};

&qupv3_se1_i2c {
	touchscreen@50 {
		imagis,power-gpioen = <1>;
		imagis,power-gpio = <&tlmm 93 0>;
		imagis,bringup = <1>;
	};
};
