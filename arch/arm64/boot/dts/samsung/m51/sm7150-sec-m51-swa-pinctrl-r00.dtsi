&soc {
	
};

&tlmm {
    usbpd_irq: usbpd_irq {
		mux {
			pins = "gpio32";
	 		function = "gpio";
	 	};
		config {
	 		pins = "gpio32";
			drive-strength = <2>;
			bias-disable;
			input-enable;
	 	};
	};
	
	if_pmic_irq: if_pmic_irq {
		mux {
			pins = "gpio68";
			function = "gpio";
		};
		config {
			pins = "gpio68";
			drive-strength = <2>;   /* 16 mA */
			bias-disable;
			input-enable;
		};
	};
	
	/* s2mu106_fg i2c */
	s2mu106_fg_i2c: s2mu106_fg_i2c {
		s2mu106_fg_active: s2mu106_fg_active {
			mux {
				pins = "gpio102", "gpio101";
				function = "gpio";
			};

			config {
				pins = "gpio102", "gpio101";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};
};

